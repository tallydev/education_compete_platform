<p id="notice"><%= notice %></p>

<h1>Listing Admin Player Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_player_feedbacks.each do |admin_player_feedback| %>
      <tr>
        <td><%= link_to 'Show', admin_player_feedback %></td>
        <td><%= link_to 'Edit', edit_admin_player_feedback_path(admin_player_feedback) %></td>
        <td><%= link_to 'Destroy', admin_player_feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Player feedback', new_admin_player_feedback_path %>
