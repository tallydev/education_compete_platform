<p id="notice"><%= notice %></p>

<h1>Listing Admin Appraises</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_appraises.each do |admin_appraise| %>
      <tr>
        <td><%= link_to 'Show', admin_appraise %></td>
        <td><%= link_to 'Edit', edit_admin_appraise_path(admin_appraise) %></td>
        <td><%= link_to 'Destroy', admin_appraise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Appraise', new_admin_appraise_path %>
