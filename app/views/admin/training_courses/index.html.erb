<p id="notice"><%= notice %></p>

<h1>Listing Admin Training Courses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_training_courses.each do |admin_training_course| %>
      <tr>
        <td><%= link_to 'Show', admin_training_course %></td>
        <td><%= link_to 'Edit', edit_admin_training_course_path(admin_training_course) %></td>
        <td><%= link_to 'Destroy', admin_training_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Training course', new_admin_training_course_path %>
