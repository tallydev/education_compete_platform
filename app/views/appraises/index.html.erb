<p id="notice"><%= notice %></p>

<h1>Listing Appraises</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @appraises.each do |appraise| %>
      <tr>
        <td><%= link_to 'Show', appraise %></td>
        <td><%= link_to 'Edit', edit_appraise_path(appraise) %></td>
        <td><%= link_to 'Destroy', appraise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Appraise', new_appraise_path %>
