<p id="notice"><%= notice %></p>

<h1>Listing School Centers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @school_centers.each do |school_center| %>
      <tr>
        <td><%= link_to 'Show', school_center %></td>
        <td><%= link_to 'Edit', edit_school_center_path(school_center) %></td>
        <td><%= link_to 'Destroy', school_center, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New School center', new_school_center_path %>
