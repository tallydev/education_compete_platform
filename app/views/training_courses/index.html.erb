<p id="notice"><%= notice %></p>

<h1>Listing Training Courses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @training_courses.each do |training_course| %>
      <tr>
        <td><%= link_to 'Show', training_course %></td>
        <td><%= link_to 'Edit', edit_training_course_path(training_course) %></td>
        <td><%= link_to 'Destroy', training_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Training course', new_training_course_path %>
