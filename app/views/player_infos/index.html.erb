<p id="notice"><%= notice %></p>

<h1>Listing Player Infos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @player_infos.each do |player_info| %>
      <tr>
        <td><%= link_to 'Show', player_info %></td>
        <td><%= link_to 'Edit', edit_player_info_path(player_info) %></td>
        <td><%= link_to 'Destroy', player_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Player info', new_player_info_path %>
