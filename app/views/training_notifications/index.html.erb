<p id="notice"><%= notice %></p>

<h1>Listing Training Notifications</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @training_notifications.each do |training_notification| %>
      <tr>
        <td><%= link_to 'Show', training_notification %></td>
        <td><%= link_to 'Edit', edit_training_notification_path(training_notification) %></td>
        <td><%= link_to 'Destroy', training_notification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Training notification', new_training_notification_path %>
