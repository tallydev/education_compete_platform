<p id="notice"><%= notice %></p>

<h1>Listing Training Course Teachers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @training_course_teachers.each do |training_course_teacher| %>
      <tr>
        <td><%= link_to 'Show', training_course_teacher %></td>
        <td><%= link_to 'Edit', edit_training_course_teacher_path(training_course_teacher) %></td>
        <td><%= link_to 'Destroy', training_course_teacher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

