<p id="notice"><%= notice %></p>

<h1>Listing Center Info Competitions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @center_info_competitions.each do |center_info_competition| %>
      <tr>
        <td><%= link_to 'Show', center_info_competition %></td>
        <td><%= link_to 'Edit', edit_center_info_competition_path(center_info_competition) %></td>
        <td><%= link_to 'Destroy', center_info_competition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

